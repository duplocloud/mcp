# DuploCloud MCP Server Configuration
# Copy this file to .env and fill in your credentials

# Required: DuploCloud API host URL
DUPLO_HOST=https://your-duplo-instance.duplocloud.net

# Required: DuploCloud API token
DUPLO_TOKEN=your-api-token-here

# Optional: Default tenant name
DUPLO_TENANT=your-tenant-name

# Optional: MCP Server port (default: 8000)
PORT=8000
